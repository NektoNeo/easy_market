{
  "$defs": {
    "FAQItem": {
      "additionalProperties": false,
      "properties": {
        "q": {
          "maxLength": 200,
          "minLength": 1,
          "title": "Q",
          "type": "string"
        },
        "a": {
          "maxLength": 600,
          "minLength": 1,
          "title": "A",
          "type": "string"
        }
      },
      "required": [
        "q",
        "a"
      ],
      "title": "FAQItem",
      "type": "object"
    },
    "Meta": {
      "additionalProperties": false,
      "properties": {
        "request_id": {
          "title": "Request Id",
          "type": "string"
        },
        "provider": {
          "title": "Provider",
          "type": "string"
        },
        "model": {
          "title": "Model",
          "type": "string"
        },
        "latency_ms": {
          "minimum": 0,
          "title": "Latency Ms",
          "type": "integer"
        }
      },
      "required": [
        "request_id",
        "provider",
        "model",
        "latency_ms"
      ],
      "title": "Meta",
      "type": "object"
    },
    "Outputs": {
      "additionalProperties": false,
      "properties": {
        "titles": {
          "items": {
            "type": "string"
          },
          "maxItems": 30,
          "title": "Titles",
          "type": "array"
        },
        "bullets": {
          "items": {
            "type": "string"
          },
          "maxItems": 30,
          "title": "Bullets",
          "type": "array"
        },
        "description_short": {
          "default": "",
          "maxLength": 2000,
          "title": "Description Short",
          "type": "string"
        },
        "description_long": {
          "default": "",
          "maxLength": 6000,
          "title": "Description Long",
          "type": "string"
        },
        "seo_keywords": {
          "items": {
            "type": "string"
          },
          "maxItems": 80,
          "title": "Seo Keywords",
          "type": "array"
        },
        "faq": {
          "items": {
            "$ref": "#/$defs/FAQItem"
          },
          "maxItems": 40,
          "title": "Faq",
          "type": "array"
        },
        "review_replies": {
          "$ref": "#/$defs/ReviewReplies"
        }
      },
      "title": "Outputs",
      "type": "object"
    },
    "ReviewReplies": {
      "additionalProperties": false,
      "properties": {
        "positive": {
          "items": {
            "type": "string"
          },
          "maxItems": 20,
          "title": "Positive",
          "type": "array"
        },
        "neutral": {
          "items": {
            "type": "string"
          },
          "maxItems": 20,
          "title": "Neutral",
          "type": "array"
        },
        "negative": {
          "items": {
            "type": "string"
          },
          "maxItems": 20,
          "title": "Negative",
          "type": "array"
        }
      },
      "title": "ReviewReplies",
      "type": "object"
    },
    "RiskFlag": {
      "additionalProperties": false,
      "properties": {
        "code": {
          "maxLength": 60,
          "minLength": 1,
          "title": "Code",
          "type": "string"
        },
        "severity": {
          "$ref": "#/$defs/RiskSeverity"
        },
        "snippet": {
          "maxLength": 200,
          "minLength": 1,
          "title": "Snippet",
          "type": "string"
        }
      },
      "required": [
        "code",
        "severity",
        "snippet"
      ],
      "title": "RiskFlag",
      "type": "object"
    },
    "RiskSeverity": {
      "enum": [
        "low",
        "medium",
        "high"
      ],
      "title": "RiskSeverity",
      "type": "string"
    }
  },
  "additionalProperties": false,
  "properties": {
    "meta": {
      "$ref": "#/$defs/Meta"
    },
    "missing_info_questions": {
      "items": {
        "type": "string"
      },
      "maxItems": 30,
      "title": "Missing Info Questions",
      "type": "array"
    },
    "risk_flags": {
      "items": {
        "$ref": "#/$defs/RiskFlag"
      },
      "maxItems": 30,
      "title": "Risk Flags",
      "type": "array"
    },
    "outputs": {
      "$ref": "#/$defs/Outputs"
    },
    "compliance_notes": {
      "items": {
        "type": "string"
      },
      "maxItems": 30,
      "title": "Compliance Notes",
      "type": "array"
    }
  },
  "required": [
    "meta",
    "outputs"
  ],
  "title": "PackagingResponse",
  "type": "object"
}
